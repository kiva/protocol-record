    "integration": "echo 'TODO: integration tests'",
    "healthcheck": "echo 'TODO: copy healthcheck to here'"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.6",
    "@nestjs/core": "^7.6.6",
    "@nestjs/platform-express": "^7.6.6",
    "@nestjs/typeorm": "^7.1.5",
    "@types/node": "^14.14.22",
    "express-jwt": "^6.0.0",
    "pg": "^8.5.1",
    "protocol-common": "^0.1.38",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.30",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.6",
    "prettier": "^2.2.1",
